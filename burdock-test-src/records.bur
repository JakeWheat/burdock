
#|

   In teaberry, these things {a:1, b:'two'} are called records, not
   objects. They will be used for 'extensible records'/'r-tuples'.
   

|#

check:
  x = 33
  a = {a: "one", b : 2, c : x }
  a.a is "one"
  a.b is 2
  a.c is 33

  is-record(a) is true

  myobj3 = {}
  is-record(myobj3) is true


  my-obj = {s: "Hello", b: true, n: 42}
  # todo: get this working or a variation when implement subtyping or something
  #fun g(thing :: {n :: Number}) -> Number:
  fun g(thing) -> Number:
    thing.n
  end

  g(my-obj) is 42

  # check non ordering of fields in equality

  {a:1, b:2} is { b:2, a:1}
end

